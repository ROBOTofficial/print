{
	"name": "gh-printer",
	"description": "Outputs the results of the actions to the specified file.",
	"version": "1.0.3",
	"module": "dist/index.js",
	"type": "module",
	"private": true,
	"license": "MIT",
	"scripts": {
		"ci-test": "npm run build && npm test",
		"build": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "jest",
		"changesets": "npx @changesets/cli",
		"actions": "echo \"Created by gh-printer.\""
	},
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@actions/exec": "^1.1.1",
		"@actions/github": "^6.0.0",
		"@changesets/cli": "^2.29.3",
		"@eslint/compat": "^1.2.9",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.26.0",
		"@jest/globals": "^29.7.0",
		"@rollup/plugin-commonjs": "^28.0.3",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-typescript": "^12.1.2",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.15.3",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jest": "^28.11.0",
		"eslint-plugin-prettier": "^5.3.1",
		"fs-extra": "^11.3.0",
		"globals": "^16.1.0",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.4.1",
		"rollup": "^4.40.1",
		"ts-jest": "^29.3.2",
		"ts-jest-resolver": "^2.0.1",
		"tslib": "^2.8.1"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "*"
	}
}
